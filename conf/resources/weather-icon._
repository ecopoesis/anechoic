<div class="icon">
    <ul>
        <% if (isDay) { %>
            <% if (image == 'chanceflurries') { %>
                <li class="wicon-frosty wicon-sunny"></li><li class="basecloud"></li>
            <% } else if (image == 'chancerain') { %>
                <li class="wicon-drizzle wicon-sunny"></li><li class="basecloud"></li>
            <% } else if (image == 'chancesleet') { %>
                <li class="wicon-sleet wicon-sunny"></li><li class="basecloud"></li>
            <% } else if (image == 'chancesnow') { %>
                <li class="wicon-snowy wicon-sunny"></li><li class="basecloud"></li>
            <% } else if (image == 'chancetstorms') { %>
                <li class="wicon-thunder wicon-sunny"></li><li class="basecloud"></li>
            <% } else if ((image == 'clear') || (image == 'sunny')) { %>
                <li class="wicon-sun"></li>
            <% } else if (image == 'cloudy') { %>
                <li class="wicon-cloud"></li>
            <% } else if (image == 'flurries') { %>
                <li class="wicon-frosty"></li><li class="basecloud"></li>
            <% } else if ((image == 'fog') || (image == 'haze')) { %>
                <li class="wicon-mist"></li>
            <% } else if ((image == 'mostlycloudy') || (image == 'mostlysunny') || (image == 'partlycloudy') || (image == 'partlysunny')) { %>
                <li class="wicon-sunny"><li class="basefullcloud"></li></li>
            <% } else if (image == 'sleet') { %>
                <li class="wicon-sleet"></li><li class="basecloud"></li>
            <% } else if (image == 'rain') { %>
                <li class="wicon-drizzle"></li><li class="basecloud"></li>
            <% } else if (image == 'snow') { %>
                <li class="wicon-snowy"></li><li class="basecloud"></li>
            <% } else if (image == 'thunder' || (image == 'tstorms')) { %>
                <li class="wicon-thunder"></li><li class="basecloud"></li>
            <% } %>
        <% } else { %>
            <% if (image == 'chanceflurries') { %>
                <li class="wicon-frosty wicon-night"></li><li class="basecloud"></li>
            <% } else if (image == 'chancerain') { %>
                <li class="wicon-drizzle wicon-night"></li><li class="basecloud"></li>
            <% } else if (image == 'chancesleet') { %>
                <li class="wicon-sleet wicon-night"></li><li class="basecloud"></li>
            <% } else if (image == 'chancesnow') { %>
                <li class="wicon-snowy wicon-night"></li><li class="basecloud"></li>
            <% } else if (image == 'chancetstorms') { %>
                <li class="wicon-thunder wicon-night"></li><li class="basecloud"></li>
            <% } else if ((image == 'clear') || (image == 'sunny')) { %>
                <li class="wicon-moon"></li>
            <% } else if (image == 'cloudy') { %>
                <li class="wicon-cloud"></li>
            <% } else if (image == 'flurries') { %>
                <li class="wicon-frosty"></li><li class="basecloud"></li>
            <% } else if ((image == 'fog') || (image == 'haze')) { %>
                <li class="wicon-mist"></li>
            <% } else if ((image == 'mostlycloudy') || (image == 'mostlysunny') || (image == 'partlycloudy') || (image == 'partlysunny')) { %>
                <li class="wicon-night"></li><li class="basefullcloud"></li>
            <% } else if (image == 'sleet') { %>
                <li class="wicon-sleet"></li><li class="basecloud"></li>
            <% } else if (image == 'rain') { %>
                <li class="wicon-drizzle"></li><li class="basecloud"></li>
            <% } else if (image == 'snow') { %>
                <li class="wicon-snowy"></li><li class="basecloud"></li>
            <% } else if (image == 'thunder' || (image == 'tstorms')) { %>
                <li class="wicon-thunder"></li><li class="basecloud"></li>
            <% } %>
        <% } %>
    </ul>
</div>